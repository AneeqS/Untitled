<% include partials/header %>

<div class="container">

    <header class="jumbotron">

        <div class="container">
            <h1>Welcome!!!!</h1>
            <p>
                <a class="btn btn-primary btn-lg" href="/campgrounds/new">Add New Campground!</a>
            </p>
        </div>

    </header>

    <div class="row">

        <% campgrounds.forEach(function(campgrounds){ %>
        <div class="col-lg-12">
            <h4> <%= campgrounds.name %> </h4>
            <img src= "<%= campgrounds.image%>">
        </div>
        <%})%>

    </div>



</div>


<% include partials/footer %>